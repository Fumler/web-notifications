{
  "name": "web-notifications",
  "version": "1.0.0",
  "description": "Website that lets an admin send a message to all its users. Minimum viable product-ish.",
  "scripts": {
    "start": "node server",
    "build": "cross-env NODE_ENV=production webpack",
    "build:clean": "del \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "yarn run build:clean && yarn run build:copy"
  },
  "private": true,
  "repository": "git@github.com:Fumler/web-notifications.git",
  "author": "Fredrik Pettersen <fredrik.fumler@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.4",
    "del-cli": "^0.2.1",
    "html-webpack-plugin": "^2.26.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "styled-components": "^1.3.1"
  }
}
